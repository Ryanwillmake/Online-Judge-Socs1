#include<stdio.h>
int main(){
	int n;
	scanf("%d", &n);
	int view[n];
	
	for(int i=0;i<n;i++){
		scanf("%d", &view[i]);getchar();
	}
	
	int q;
	scanf("%d", &q);
	for(int i=1;i<=q;i++){
		int a,b;
		scanf("%d %d", &a,&b);
		int sum;
		for(int j=(a-1);j<b;j++){
		sum += view[j];
			
		}
		printf("Case #%d: %d\n", i, sum);
	}
	
}
